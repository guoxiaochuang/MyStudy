<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
</head>
<script type="text/javascript">
	var i = 5;
	document.write('switch语句：');
	document.write('<br/>');
	switch (i){
		case 1:
			document.write('i='+1);
			break;//跳出当前
		case 2:
			document.write('i='+2);
			break;//跳出当前
		default:
			document.write('条件不满足case');
			break;
	}
	document.write('<br/><hr/>');
	document.write('循环语句，for语句：');
	document.write('<br/>');
	var a = [1,2,3,4,5];
	for(var i=0;i<a.length;i++){
		document.write(a[i]);
		document.write('<br/>');
	}
	for(i in a){
		document.write(a[i]);
		document.write('<br/>');
	}
	document.write('<br/><hr/>');
	document.write('循环语句，while语句：');
	document.write('<br/>');
	var i = 1;
	while(i<10){
		document.write('i='+i);
		document.write('<br/>');
		i++;
	}
	document.write('循环语句，do...while语句：');
	document.write('<br/><hr/>');
	var i = 1;
	do{
		document.write('i='+i);
		document.write('<br/>');
		i++;
	}while(i<10){
	}
</script>
<body>

</body>
</html>